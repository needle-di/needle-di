{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "format": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "test:ci": {
      "dependsOn": ["^test:ci"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    }
  }
}
